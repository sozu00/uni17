public class EliminadorVacias {
	

	public ArrayList<String> eliminarVacias(ArrayList<String> vText) throws IOException{
		String p = new String(Files.readAllBytes(Paths.get(AppPath.Empty)));
		ArrayList<String> palabras = new 
				ArrayList<String>(Arrays.asList(p.split("\n")));
		
		
		return vText;
	}
}
